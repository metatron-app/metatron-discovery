{
  "name": "polaris2-ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=10240 ./node_modules/.bin/ng serve --proxy-config proxy.conf.js --live-reload false --host 0.0.0.0 --disable-host-check",
    "build": "npm run package-reinstall && ng build --base-href /app/v2/",
    "package-reinstall": "rm -f package-lock.json && npm install",
    "prod": "npm run package-reinstall && node --max_old_space_size=10240 ./node_modules/.bin/ng build --deploy-url /resource/ --output-hashing all --prod --base-href /app/v2/",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e",
    "dummy-db": "json-server --watch dummy.json",
    "prebuild": "node prebuild.js"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "11.2.8",
    "@angular/cdk": "^11.2.9",
    "@angular/common": "11.2.8",
    "@angular/compiler": "11.2.8",
    "@angular/core": "11.2.8",
    "@angular/forms": "11.2.8",
    "@angular/platform-browser": "11.2.8",
    "@angular/platform-browser-dynamic": "11.2.8",
    "@angular/router": "11.2.8",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "@stomp/ng2-stompjs": "7.2.0",
    "@turf/turf": "5.1.6",
    "air-datepicker": "2.2.3",
    "antlr4": "4.9.2",
    "codemirror": "5.59.3",
    "core-js": "3.10.0",
    "dragula": "3.7.2",
    "fast-text-encoding": "1.0.0",
    "file-saver": "1.3.8",
    "h3-js": "3.2.0",
    "html2canvas": "1.0.0-alpha.12",
    "inputmask": "4.0.6",
    "jquery": "^3.5.1",
    "json5": "1.0.1",
    "jspdf": "^2.3.0",
    "lodash": "^4.17.15",
    "moment": "2.29.1",
    "ng-click-outside": "2.4.1",
