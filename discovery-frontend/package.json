    "@angular/compiler-cli": "6.1.10",
    "@angular/language-service": "6.1.10",
    "@types/codemirror": "0.0.55",
    "@types/echarts": "0.0.6",
    "@types/jasmine": "2.5.45",
    "@types/jquery": "3.3.29",
    "@types/lodash": "4.14.119",
