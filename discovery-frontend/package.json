{
  "name": "polaris2-ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=10240 ./node_modules/.bin/ng serve --proxy-config proxy.conf.js --live-reload false --host 0.0.0.0 --disable-host-check",
    "build": "npm run package-reinstall && ng build --base-href /app/v2/",
    "package-reinstall": "rm -f package-lock.json && npm install",
    "prod": "npm run package-reinstall && node --max_old_space_size=10240 ./node_modules/.bin/ng build --deploy-url /resource/ --output-hashing all --prod --base-href /app/v2/",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e",
    "dummy-db": "json-server --watch dummy.json",
    "prebuild": "node prebuild.js"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "6.1.10",
    "@angular/common": "6.1.10",
    "@angular/compiler": "6.1.10",
    "@angular/core": "6.1.10",
    "@angular/forms": "6.1.10",
    "@angular/http": "6.1.10",
    "@angular/platform-browser": "6.1.10",
    "@angular/platform-browser-dynamic": "6.1.10",
    "@angular/router": "6.1.10",
    "@ngx-translate/core": "10.0.2",
    "@ngx-translate/http-loader": "3.0.1",
    "@stomp/ng2-stompjs": "7.2.0",
    "@turf/turf": "5.1.6",
    "air-datepicker": "2.2.3",
    "angular2-moment": "1.9.0",
    "antlr4": "4.7.2",
    "core-js": "2.6.1",
    "dragula": "3.7.2",
    "fast-text-encoding": "1.0.0",
    "file-saver": "1.3.8",
    "h3-js": "3.2.0",
    "html2canvas": "1.0.0-alpha.12",
    "inputmask": "4.0.6",
    "jquery": "3.4.1",
    "json5": "1.0.1",
    "lodash": "^4.17.15",
    "ng-click-outside": "2.4.1",
    "ng2-codemirror": "1.1.3",
