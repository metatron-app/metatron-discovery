    "@angular-devkit/build-angular": "0.1102.7",
    "@angular/cli": "11.2.7",
    "@angular/compiler-cli": "11.2.8",
    "@angular/language-service": "6.1.10",
    "@types/codemirror": "0.0.55",
    "@types/echarts": "0.0.6",
    "@types/jasmine": "2.5.45",
    "@types/jquery": "3.3.29",
