{
  "name": "polaris2-ui",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "node --max_old_space_size=10240 ./node_modules/.bin/ng serve --local --proxy-config proxy.conf.js --live-reload false --host 0.0.0.0 --disable-host-check",
    "build": "npm run package-reinstall && ng build --base-href /app/v2/",
    "package-reinstall": "rm -f package-lock.json && npm install",
    "prod": "npm run package-reinstall && node --max_old_space_size=10240 ./node_modules/.bin/ng build -d /resource/ --output-hashing all --prod --env=prod --no-extract-license --base-href /app/v2/",
    "test": "ng test",
    "lint": "ng lint",
    "lint-fix": "ng lint --fix",
    "e2e": "ng e2e",
    "dummy-db": "json-server --watch dummy.json",
    "prebuild": "node prebuild.js"
  },
  "private": true,
  "dependencies": {
